{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": [
			"react-app", "prettier"
	],
	"parserOptions": {
			"ecmaVersion": 12
	},
	"plugins": [
			"react",
			"prettier"
	],
	"rules": {
			"prettier/prettier": [
					"error",
			{
					"no-unused-vars": "warn",
					"printWidth": 80,
					"no-console": "warn",
					"func-names": "off",
					"useTabs": true,
					"endOfLine": "auto"
			}
			]
	}
}
